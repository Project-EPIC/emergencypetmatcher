upstream backend {
	server 127.0.0.1:8080;
}

server {
	listen 80;
	server_name www.barronsoftware.com barronsoftware.com;

	access_log /home/jbarron/public_html/barronsoftware.com/logs/nginx_access.log;
	error_log /home/jbarron/public_html/barronsoftware.com/logs/nginx_error.log;

    location /nplh/static/ {
        alias /home/jbarron/public_html/barronsoftware.com/static/;
    }
    location /nplh/ {
		proxy_pass http://backend;
		include /etc/nginx/proxy.conf;
	}

	root /home/jbarron/public_html/barronsoftware.com/static/;
}

